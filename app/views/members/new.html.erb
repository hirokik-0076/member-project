<h1>アカウント作成</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@member,url:signup_path) do |f| %>
      <%= render 'shared/error_messages' %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :profile %>
      <%= f.text_field :profile, class: 'form-control' %>

      <%= f.label :skill %>
      <%= f.text_field :skill, class: 'form-control' %>

      <%= f.label :project %>
      <%= f.text_field :project, class: 'form-control' %>

      <%= f.label :coment %>
      <%= f.text_field :coment, class: 'form-control' %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>