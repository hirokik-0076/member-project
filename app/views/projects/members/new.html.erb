<h1>メンバー追加</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with model: [@project, @member], local: true do |f| %>
      <%= f.label :ID %>
      <%= f.number_field :id, class: 'form-control' %>
      <%= f.submit "Relate account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>